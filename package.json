{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "test": "jest --watchAll",
    "postinstall": "patch-package && jetify",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean_install_macos": "watchman watch-del-all && rm -rf node_modules && npm i --legacy-peer-deps && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean_install_windows": "npm i rimraf --no-save && npx rimraf node_modules && npm i --legacy-peer-deps"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-firebase/app": "^12.4.0",
    "@react-native-firebase/messaging": "^12.4.0",
    "@react-native-picker/picker": "^1.16.4",
    "axios": ">=0.21.1",
    "btoa": "^1.2.1",
    "node-forge": "^0.10.0",
    "react": "17.0.2",
    "react-native": "0.64.2",
    "react-native-camera": "^4.0.0",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-modal": "^12.0.3",
    "react-native-permissions": "^3.0.5",
    "react-native-qrcode-scanner": "^1.5.4",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": ">=11.6.5",
    "react-navigation": "^4.x",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@react-native-community/eslint-config": "^3.0.0",
    "babel-jest": "27.0.6",
    "jest": "^27.0.6",
    "jetifier": "^2.0.0",
    "node-notifier": ">=10.0.0",
    "patch-package": "^6.4.7",
    "react-test-renderer": "17.0.2"
  },
