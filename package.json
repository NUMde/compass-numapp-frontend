{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose",
    "coverage": "jest --collectCoverage",
    "postinstall": "patch-package && jetify",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean_install_macos": "watchman watch-del-all && rm -rf node_modules && npm i --legacy-peer-deps && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean_install_windows": "npm i rimraf --no-save && npx rimraf node_modules && npm i --legacy-peer-deps",
    "lint": "eslint  . --ext .js,.jsx",
    "lint:fix": "eslint  . --ext .js,.jsx --fix",
    "lint:report": "eslint . --ext .js,.jsx --output-file eslint_report.json --format json ."
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "^4.1.6",
    "@react-native-firebase/app": "^12.7.5",
    "@react-native-firebase/messaging": "^12.7.5",
    "@react-native-picker/picker": "^2.0.0",
    "axios": ">=0.21.4",
    "buffer": "^6.0.3",
    "node-forge": "^0.10.0",
    "react": "^17.0.1",
    "react-native": "0.64.2",
    "react-native-camera": "^4.1.1",
    "react-native-elements": "^3.4.2",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-modal": "^13.0.0",
    "react-native-permissions": "^3.0.5",
    "react-native-qrcode-scanner": "^1.5.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": ">=11.13.0",
    "react-navigation": "^4.x",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/react-native": "^7.2.0",
    "babel-jest": "26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "jest": "^26.6.3",
    "jetifier": "^2.0.0",
    "jsdom": "17.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "patch-package": "^6.4.7",
    "react-test-renderer": "17.0.2"
  },
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
