    "@testing-library/react-native": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^7.0.0",
    "eslint-formatter-github-annotations": "^0.1.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
