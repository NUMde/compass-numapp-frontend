{
  "name": "Compass",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose",
    "coverage": "jest --collectCoverage",
    "postinstall": "patch-package && node ./scripts/postInstall.js",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean_install_macos": "watchman watch-del-all && rm -rf node_modules && npm i --legacy-peer-deps && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean_install_windows": "npm i rimraf --no-save && npx rimraf node_modules && npm i --legacy-peer-deps",
    "lint": "eslint  . --ext .js,.jsx",
    "lint:fix": "eslint  . --ext .js,.jsx --fix",
    "lint:report": "eslint . --ext .js,.jsx --output-file eslint_report.json --format json .",
    "lint:annotate": "eslint -f github-annotations . --ext .js,.jsx"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "6.1.3",
    "@react-native-community/slider": "4.2.2",
    "@react-native-firebase/app": "14.11.0",
    "@react-native-firebase/messaging": "14.11.0",
    "@react-native-picker/picker": "2.4.1",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.2",
    "@redux-devtools/extension": "3.2.2",
    "@reduxjs/toolkit": "1.8.2",
    "babel-plugin-module-resolver": "4.1.0",
    "buffer": "6.0.3",
    "i18n-js": "3.9.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.memoize": "4.1.2",
    "node-forge": "1.3.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-camera": "4.2.1",
    "react-native-elements": "3.4.2",
    "react-native-encrypted-storage": "4.0.2",
    "react-native-gesture-handler": "2.4.2",
    "react-native-localize": "2.2.2",
    "react-native-modal": "13.0.1",
    "react-native-permissions": "3.3.1",
    "react-native-qrcode-scanner": "1.5.5",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "3.13.1",
    "react-native-splash-screen": "3.3.0",
    "react-native-url-polyfill": "1.3.0",
    "react-native-vector-icons": "9.1.0",
    "react-native-webview": "11.21.1",
    "react-redux": "8.0.2",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@react-native-community/eslint-config": "3.0.2",
    "@testing-library/react-native": "9.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-github-annotations": "0.1.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "7.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "jest-transform-stub": "2.0.0",
    "jsdom": "19.0.0",
    "metro-react-native-babel-preset": "0.68.0",
    "msw": "0.42.0",
    "patch-package": "6.4.7",
    "react-test-renderer": "17.0.2"
  },
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
