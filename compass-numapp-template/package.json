{
  "name": "Compass",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose",
    "coverage": "jest --collectCoverage",
    "postinstall": "patch-package",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean_install_macos": "watchman watch-del-all && rm -rf node_modules && npm i --legacy-peer-deps && cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean_install_windows": "npm i rimraf --no-save && npx rimraf node_modules && npm i --legacy-peer-deps",
    "lint": "eslint  . --ext .js,.jsx",
    "lint:fix": "eslint  . --ext .js,.jsx --fix",
    "lint:report": "eslint . --ext .js,.jsx --output-file eslint_report.json --format json .",
    "lint:annotate": "eslint -f github-annotations . --ext .js,.jsx"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "7.4.1",
    "@react-native-community/slider": "4.4.3",
    "@react-native-firebase/app": "18.3.0",
    "@react-native-firebase/messaging": "18.3.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@reduxjs/toolkit": "1.9.7",
    "babel-plugin-module-resolver": "5.0.2",
    "buffer": "6.0.3",
    "i18n-js": "3.9.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.memoize": "4.1.2",
    "node-forge": "1.3.1",
    "prop-types": "15.8.1",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-elements": "3.4.3",
    "react-native-encrypted-storage": "4.0.3",
    "react-native-gesture-handler": "2.12.0",
    "react-native-localize": "3.0.2",
    "react-native-modal": "13.0.1",
    "react-native-reanimated": "3.16.3",
    "react-native-safe-area-context": "4.7.1",
    "react-native-screens": "3.25.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-url-polyfill": "2.0.0",
    "react-native-vector-icons": "10.2.0",
    "react-native-vision-camera": "4.6.3",
    "react-native-webview": "13.3.0",
    "react-redux": "8.1.3",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.2",
    "vision-camera-code-scanner": "0.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "7.23.2",
    "@react-native-community/eslint-config": "3.2.0",
    "@testing-library/react-native": "12.1.3",
    "babel-jest": "29.7.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.10.0",
    "eslint-formatter-github-annotations": "0.1.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "jsdom": "22.1.0",
    "metro-react-native-babel-preset": "0.77.0",
    "msw": "1.2.3",
    "patch-package": "8.0.0",
    "react-test-renderer": "18.1.0"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
